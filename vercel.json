{
    "version": 2,
    "builds": [
      {
        "src": "app.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "app.js",
        "headers": {
          "Access-Control-Allow-Origin": "*",
          "Access-Control-Allow-Methods": "GET,POST,PUT,DELETE,OPTIONS",
          "Access-Control-Allow-Headers": "X-Requested-With, Content-Type, Authorization"
        }
      }
    ],
    "functions": {
      "app.js": {
        "memory": 1024,
        "maxDuration": 60
      }
    }
  }